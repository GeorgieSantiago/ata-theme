<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column"
xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body htmlClass="container">
        <referenceContainer name="header-menu-wrapper" >
        </referenceContainer>
        <referenceContainer name="header-menu-wrapper" htmlClass="stickyribbon d-flex flex-row justify-content-md-start justify-content-end ata-nav-spacing" />
        <referenceBlock name="titleBlock" remove="true" />
        <referenceBlock name="logo" remove="true" />
        <referenceBlock name="header-wrapper" remove="true" />
        <!--<referenceBlock name="minicart" remove="true" />-->
        <referenceBlock name="compare.trigger" remove="true" />
        <move element="rmi.street.address" destination="content" />
        <move before="-" element="breadcrumbs" destination="div.sidebar.main" />
        <container name="ata.footer.wrapper" htmlTag="div" htmlClass="ata-footer-wrapper" />
        <move element="footer" destination="ata.footer.wrapper" />-->
        <move element="minicart" destination="ata.header.right" />
    </body>
</page>
